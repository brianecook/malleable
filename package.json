{
  "name": "shopify-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "moduleNameMapper": {
    "^react/jsx-runtime$": "preact/jsx-runtime"
  },
  "eslintConfig": {
    "extends": "eslint-config-wesbos/typescript.js",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "class-methods-use-this": "off"
    },
    "ignorePatterns": [
      "jest.setup.js"
    ],
    "settings": {
      "import/resolver": {
        "alias": {
          "map": [
            [
              "@styles",
              "./styles"
            ],
            [
              "@scripts",
              "./scripts"
            ]
          ]
        }
      }
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom",
      "<rootDir>/jest.setup.js"
    ]
  },
  "scripts": {
    "start": "shopify theme dev --store=eloquent-development --theme-editor-sync",
    "watch": "webpack --watch",
    "analyze": "webpack-bundle-analyzer stats.json",
    "push": "shopify theme push",
    "build": "webpack --mode production",
    "test": "jest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-transform-modules-commonjs": "^7.21.5",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.18.6",
    "@chromatic-com/storybook": "^1.3.2",
    "@storybook/addon-essentials": "^8.0.8",
    "@storybook/addon-interactions": "^8.0.8",
    "@storybook/addon-links": "^8.0.8",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.2",
    "@storybook/blocks": "^8.0.8",
    "@storybook/preact": "^8.0.8",
    "@storybook/preact-webpack5": "^8.0.8",
    "@storybook/test": "^8.0.8",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.11.13",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.2",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-wesbos": "^3.1.4",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "glob": "^10.3.12",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.0",
    "prettier": "^2.8.1",
    "react": "^18.3.1",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "sass-resources-loader": "^2.2.5",
    "storybook": "^8.0.8",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@react-icons/all-files": "^4.1.0",
    "preact": "^10.20.1",
    "selectricity": "^1.2.0"
  }
}
