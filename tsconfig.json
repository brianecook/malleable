{
  "extends": "eslint-config-wesbos/tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "es6",
    "module": "commonjs",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types", "./scripts/types"]
  },
  "include": [
    "./scripts/**/*.ts",
    "./scripts/**/*.tsx"
  ]
}